  "module": "dist/index.es.js",
  "license": "MIT",
  "maintainers": [
    {
      "name": "Thomas Gafner",
      "email": "thomas.gafner@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/thomasgafner/prosemirror-selection-markers.git"
  },
